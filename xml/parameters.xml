<?xml version="1.0" encoding="utf-8"?>
<euler_problems>
	<problem id="1">
		<description>
			Takes two natural numbers and a natural limit and calculates the 
			addition of the multiples of either number.
		</description>
		<parameters count="3">
			<parameter>3</parameter>
			<parameter>5</parameter>
			<parameter>999</parameter>
		</parameters>
	</problem>
	<problem id="2">
		<description>
			Takes a limit up to which the addition of the even Fibonacci numbers
			must be calculated.
		</description>
		<parameters count="1">
			<parameter>4000000</parameter>
		</parameters>
	</problem>
	<problem id="3">
		<description>
			Finds the largest primer factor of a number.
		</description>
		<parameters count="1">
			<parameter>600851475143</parameter>
		</parameters>
	</problem>
	<problem id="4">
		<description>
			Finds the largest palindrome of the factor of two n-digit numbers.
		</description>
		<parameters count="2">
			<parameter>100</parameter>
			<parameter>999</parameter>
		</parameters>
	</problem>
	<problem id="5">
		<description>
			Finds the smallest natural number divisible by a sequence of numbers.
		</description>
		<parameters count="2">
			<parameter>1</parameter>
			<parameter>20</parameter>
		</parameters>
	</problem>
	<problem id="6">
		<description>
			Finds the difference between the square of the sum and the sum of the
			squares within the given limits.
		</description>
		<parameters count="2">
			<parameter>1</parameter>
			<parameter>100</parameter>
		</parameters>
	</problem>
	<problem id="7">
		<description>
			Finds the nth prime number.
		</description>
		<parameters count="1">
			<parameter>10001</parameter>
		</parameters>
	</problem>
	<problem id="8">
		<description>
			Finds the greatest product in a consecutive subset of numbers.
		</description>
		<parameters count="2">
			<parameter>13</parameter>
			<parameter></parameter>
		</parameters>
	</problem>
	<problem id="9">
		<description>
			Finds the Pythagorean triplet for which a + b + c = 1000.
		</description>
		<parameters count="0"/>
	</problem>
	<problem id="10">
		<description>
			Finds the sum of primes under the limit.
		</description>
		<parameters count="1">
			<parameter>2000000</parameter>
		</parameters>
	</problem>
</euler_problems>